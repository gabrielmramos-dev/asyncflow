generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Video {
  id          String   @id @default(uuid())
  videoName   String
  status      String   @default("PENDING") // PENDING, PROCESSING, COMPLETED, FAILED
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}